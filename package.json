{
  "name": "three-tape-tm",
  "version": "0.5.0",
  "description": "Turing machine visualization and simulator with three tapes.",
  "homepage": "https://github.com/3TapeMachine/3TapeMachine",
  "license": "BSD-3-Clause",
  "contributors": [
    {
      "name": "Andy Li",
      "email": "andy.srs.li@gmail.com"
    },
    {
      "name": "Samyek Ofori-Addo",
      "email": "oforiadds@jacks.sfasu.edu"
    },
    {
      "name": "Billy Harris",
      "email": "billy.harris@sfasu.edu"
    }
  ],
  "repository": "github:3TapeMachine/3TapeMachine",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "trash build/ || rm -r build/",
    "depgraph": "mkdir -p build/ && (cd src/ && madge . --dot) > build/depgraph.gv",
    "depgraph-noext": "mkdir -p build/ && (cd src/ && madge . --dot -x \"`node -e \"console.log(Object.keys(require('../webpack.config').externals).map(s => '^'+s+'$').join('|'))\"`\") > build/depgraph-noext.gv",
    "lint": "eslint --cache webpack.config.js src/",
    "prod": "export NODE_ENV=production; npm run lint && webpack --progress",
    "start": "webpack-dev-server --progress --stats-error-details --host=0.0.0.0",
    "watch": "webpack --watch --progresss"
  },
  "dependencies": {
    "ace-builds": "^1.42.0",
    "bootstrap": "^5.3.6",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "d3": "^7.9.0",
    "eslint": "^9.28.0",
    "js-yaml": "^4.1.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-dev-server": "^5.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "globals": "^16.2.0",
    "webpack-cli": "^6.0.1"
  }
}

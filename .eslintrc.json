{
    "rules": {
        "indent": [
            1,
            2,
            {
              "SwitchCase": 1,
              "VariableDeclarator": {"var": 2}
            }
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ],
        "no-unexpected-multiline": 2
    },
    "env": {
        "commonjs": true
    },
    "extends": "eslint:recommended"
}
